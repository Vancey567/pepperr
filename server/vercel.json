{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      },
      {
        "src": "/(.*)",
        "dest": "/",
        "use": "@vercel/npm",
        "config": {
          "buildCommand": "npm run build",
          "output": "dist"
        }
      }
    ]
  }
  